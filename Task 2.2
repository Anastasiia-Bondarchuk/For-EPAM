package com.company;
import java.util.Scanner;

public class Main {
    public static void ArrOutput(int n, int arr[])
    {
        for (int i=0;i<n;i++)
        {
            System.out.printf("%d ", arr[i]);
        }
    }

    public static void main(String[] args)
    {
        Scanner in=new Scanner(System.in);
        System.out.print("Input a number: ");
        int n = in.nextInt();
        int count=0;
        int []arr = new int[n];
        for (int i=0;i<n;i++)
        {
            arr[i]=(int)Math.round(Math.random()*30);
        }
        System.out.printf("{ ");
        ArrOutput(n, arr);
        System.out.printf(" }");
        for (int i=0;i<n;i++)
        {
            for (int j=i+1;j<n;j++)
            {
                if (arr[i]==arr[j])
                    count++;
            }
        }

        System.out.println("\n\nResult of array:");
        System.out.printf("{ ");
        ArrOutput(n, arr);
        System.out.printf(" } = %d", count);

    }
}
